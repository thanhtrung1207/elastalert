pipline {
    agent any
    
    stages{
        stage('Checkout'){
            steps{
                script{
                    git url: 'https://10.96.24.141/UDTN/integration/microservices/data-streaming/mcs-elasticalert.git', branch: 'dev'
                }
            }
        }
        stage('Build and Package'){
            steps{
                script{
                    sh 'pip install -r requirement.txt'
                }
            }
        }
    }
}